{
    "name": "onramplab/laravel-security-model",
    "description": "A Laravel package providing security for Eloquent model",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "security",
        "encryption",
        "eloquent",
        "model"
    ],
    "authors": [
        {
            "name": "OnrampLab",
            "email": "dev@onramplab.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "aws/aws-sdk-php": "^3.258",
        "illuminate/database": "^8.0|^9.0",
        "paragonie/ciphersweet": "^3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "nunomaduro/larastan": "^1.0",
        "nunomaduro/phpinsights": "^2.6",
        "onramplab/onr-phpcs-laravel": "^1.2",
        "orchestra/testbench": "^6.25",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15.3",
        "sempro/phpunit-pretty-print": "^1.4",
        "spatie/phpunit-watcher": "^1.23",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "OnrampLab\\SecurityModel\\": "src/",
            "OnrampLab\\SecurityModel\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnrampLab\\SecurityModel\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "OnrampLab\\SecurityModel\\SecurityModelServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch --filter NoneTest < /dev/tty"
        ],
        "psr2check": [
            "@php vendor/bin/phpcs --standard=PSR2 src/"
        ],
        "psr2autofix": [
            "@php vendor/bin/phpcbf --standard=PSR2 src/"
        ],
        "docs": [
            "@php tools/phpDocumentor"
        ],
        "insights": [
            "@php vendor/bin/phpinsights analyse --no-interaction src"
        ],
        "insights:fix": [
            "@php vendor/bin/phpinsights analyse --no-interaction --fix src"
        ],
        "dependencies:check": [
            "phpmd src text rulesets.xml"
        ],
        "rector": [
            "vendor/bin/rector process src --dry-run"
        ],
        "rector:fix": [
            "vendor/bin/rector process src"
        ]
    }
}
